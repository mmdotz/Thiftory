<p id="notice"><%= notice %></p>

<h1>Listing Subcat2s</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Subcat1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subcat2s.each do |subcat2| %>
      <tr>
        <td><%= subcat2.id %></td>
        <td><%= subcat2.name %></td>
        <td><%= subcat2.subcat1.id %></td>
        <td><%= subcat2.subcat1.name %></td>
        <td><%= link_to 'Show', subcat2 %></td>
        <td><%= link_to 'Edit', edit_subcat2_path(subcat2) %></td>
        <td><%= link_to 'Destroy', subcat2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

